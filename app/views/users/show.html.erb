<%= content_tag(:div, content_tag(:a, 'Edit Profile', href: edit_user_registration_path)) %>
<%= render 'user_card', user: @user %>
<%= content_tag(:div, tag.p(@current_user.bio), class: 'card__box') %>
<%= render partial: 'posts/post_card', collection: @current_user.most_recent_posts, as: :post, locals: { detailed: false } %>
<%= link_to 'See all posts', user_posts_path(@current_user.id) %>
